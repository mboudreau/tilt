!= messages()

h2= title

div#users
  - if(users.length)
    table.users_list
      tr
        th username
        th email
        th funds
        th team
        th actions 
      != partial('user_list', {collection: users, as: 'aUser'})
  - else
    #no-results No users found. Register one here: &nbsp
    a(href='/users/register') here
